{
  "name": "griddle-overhaul-react-redux",
  "version": "0.1.5",
  "description": "A fast and flexible grid component for React",
  "keywords": [
    "react-component",
    "grid",
    "react",
    "pagination",
    "sort"
  ],
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "babel src/lib -d lib",
    "watch": "npm run build -- --watch",
    "predoc": "rimraf doc",
    "doc": "esdoc -c ./esdoc.json",
    "prerelease": "npm run build",
    "release": "npm version patch && npm publish",
    "postrelease": "npm run release-doc",
    "prerelease-doc": "npm run doc",
    "release-doc": "git subtree push --prefix doc origin gh-pages",
    "postrelease-doc": "git commit -am \"doc-release\" && git push --follow-tags",
    "start": "node server.js",
    "dist-debug": "./node_modules/.bin/webpack --config webpack.config.js",
    "dist": "./node_modules/.bin/webpack --config webpack-dist.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.7.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^4.3.0",
    "rimraf": "^2.5.2",
    "node-libs-browser": "^0.5.3",
    "webpack": "*",
    "webpack-dev-server": "*"
  },
  "dependencies": {
    "esdoc-es7-plugin": "0.0.3",
    "griddle-overhaul-core": "*",
    "griddle-overhaul-react": "*",
    "griddle-overhaul-redux": "*",
    "lodash.assign": "^3.2.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "author": "Griddle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cchamberlain/griddle-overhaul.git"
  }
}
